# Application Configuration
NEXT_PUBLIC_APP_NAME=AA Virtual
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Database (for development)
DATABASE_URL=postgresql://user:password@localhost:5432/aa_virtual_dev

# Redis (for caching and rate limiting)
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_ISSUER=http://localhost:8000
JWT_AUDIENCE=aa-virtual
JWT_SIGNING_KEY=your-jwt-signing-key-here

# Email Configuration
EMAIL_SENDER=noreply@example.com
EMAIL_PROVIDER_API_KEY=your-email-provider-api-key

# Storage (S3-compatible)
S3_BUCKET=aa-virtual-dev
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key
S3_ENDPOINT_URL=https://s3.amazonaws.com

# External APIs
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
APPLE_CLIENT_ID=your-apple-client-id
APPLE_CLIENT_SECRET=your-apple-client-secret

# Monitoring
OTEL_EXPORTER_OTLP_ENDPOINT=https://api.honeycomb.io
OTEL_SERVICE_NAME=aa-virtual-frontend